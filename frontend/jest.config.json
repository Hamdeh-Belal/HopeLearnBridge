{
    "transform": {
        "^.+\\.tsx?$": ["ts-jest", { "tsconfig": "tsconfig.app.json" }]
    },
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.ts"],
    "testEnvironment": "jsdom",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "moduleDirectories": ["node_modules", "src"]
}